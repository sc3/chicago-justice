
body {
  padding-top: 0;
  overflow-x: hidden;
}

body .container {
  max-width: 1088px;
  margin-right: auto;
  margin-left: auto;
}


/* Text */

a:hover { 
  color: #ed1c24; 
}

h1, h2, h3, h4, h5, h6 {
  font-family: "Lusitana";
}

h1 {
  font-size: 4em;
}

.unavailable {
  color: #b33;
}

.foiable {
  color: gray;
}

.available {
  color: black;
}


/* Overlays */

.featured-image-wrapper h1 {
  color: #fff;
  font-size: 80px;
  text-shadow: 2px 2px 3px #000000;
  position: absolute;
  width: 100%;
  left: 0;
  bottom: 60px;
  text-align: center;
  display: none;
}

.featured-image-wrapper .scroll-injunction {
  color: #fff;
  text-shadow: 2px 2px 3px #000000;
  position: absolute;
  width: 100%;
  left: 0;
  bottom: 10px;
  text-align: center;
}

.featured-image-wrapper .scroll-injunction i {
  margin: 0 5px;
}


/* Navbar */

.circle {
  font-size: 11px;
  color: #000;
  background-color: #FFF;
  border-radius: 8px;
  height: 17px;
  width: 17px;
  line-height: 16px;
  display: inline-block;
  text-align: center;
}

.navbar {
  height: 37px;
  min-height: 37px;
  background-color: rgba(20, 20, 20, 0.92);
}

div.navbar-header {
    font-size: 18px !important;
    font-family: "Baskerville", sans-serif !important;
}

ul.sections.nav.navbar-nav {
    font-size: 12px !important;
}

.navbar-default {
  border: none;
}

.navbar-default .navbar-brand,
.navbar-default .navbar-nav>li>a { 
  color: #ddd; 
}

/*.navbar-default .navbar-nav>li>a:hover,
.navbar-default .navbar-nav>li>a.active {
  background-color: #4f4c4d;
}*/

.navbar-default .navbar-brand:hover,
.navbar-default .navbar-nav>li>a:hover, 
.navbar-default .navbar-nav>li>a.active,
.navbar-default .navbar-nav>li>a:hover>span,
.navbar-default .navbar-nav>li>a.active>span { 
  color: #fff; 
}

.navbar a:hover .circle, 
.navbar a.active .circle {
  background-color: #b33;
}


/* Footer */

#footer {
  background-color: rgba(20, 20, 20, .92);
  position: relative;
  bottom: 0;
  height: 37px;
}

#footer p {
  margin-bottom: 0;
}

#footer #next {
  position: absolute;
  right: 10%;
}

#footer #prev {
  position: absolute;
  left: 10%;
}


/* Images */

img {
  max-height: auto;
  max-width: 100%;
}

#black-hole {
  background-color: #000;
  border-radius: 220px;
  width: 440px;
  height: 440px;
  margin-top: 60px;
  margin-right: auto;
  margin-left: auto;
}

/* Table */
td {
    width: 100px;
    text-align: center;
}

.chart td, .chart th {
  height: 50px;
  text-align: center;
  font-weight: normal;
}

.chart_circle {
    width: 1%; 
    margin-left: auto;
    margin-right: auto;
    border-radius: 100%;
    background-color: #989898;
}

.circle_lots {
    padding: 16px;
}

.circle_some {
    padding: 12px;
}

.circle_little {
    padding: 8px;
}

tr.highlighted {
    background-color: #F0F0F0;
}

