/* Content */

.content { margin-top: 30px; }
.content img { max-width: 100%; }


/* Responsivity */

@media (max-width: 1000px) {
  .navbar .social-menu {
    display: none;
  }
}

@media (min-width: 768px) {
  .navbar-nav>li>a, .navbar-brand {
    padding-top: 8px;
    padding-bottom: 8px;
  }
}

@media (max-width: 768px) {
  .navbar .social-menu {
    margin: 30px 20px 30px 20px;
    display: none; 
  }
  h1 {
    font-size: 30px;
  }
  .bubble {
    max-width: 50%;
    max-height: 100%;
  }
}

@media (max-width: 482px) {
  /* Nothing */
